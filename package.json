{
  "name": "royalpalacehotel-monorepo",
  "homepage": "https://rishavdevtiwari.github.io/royalpalacehoteljanakpur",
  "private": true,
  "workspaces": [
    "apps/frontend",
    "apps/backend"
  ],
  "scripts": {
    "install": "npm install --workspaces",
    "build": "npm run build --workspace=apps/frontend",
    "start": "npm run start --workspace=apps/backend",
    "dev": "concurrently \"npm run dev --workspace=apps/frontend\" \"npm run dev --workspace=apps/backend\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "gh-pages": "^6.3.0",
    "prisma": "^6.5.0"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0"
  }
}
